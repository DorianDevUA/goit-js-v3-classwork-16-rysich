{
  "printWidth": 120,
  "proseWrap": "always",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSameLine": true,
  "jsxBracketSameLine": true,
  "endOfLine": "lf"
}
